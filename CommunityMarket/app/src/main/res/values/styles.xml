<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/blanc</item>
    </style>
    <style name="AppTheme.AppBarOverlay_two" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >
    <item name="android:fontFamily">@font/pacifico</item>
</style>
<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="slash" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@drawable/splash_activity_background</item>
    </style>

    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/pacifico</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_back_arrow</item>
    </style>
    <style name="ToolbarThemeTwo" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/pacifico</item>
        <item name="colorPrimary">@color/colorPrimary</item>
    </style>
</resources>
